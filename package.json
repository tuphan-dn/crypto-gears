{
  "name": "@gears-bot/crypto",
  "version": "1.0.1",
  "description": "A set of custom crypto protocols",
  "homepage": "https://crypto.gears.bot/",
  "keywords": [
    "sss",
    "mpc",
    "tss",
    "eddsa",
    "ecdsa",
    "ed25519",
    "secp256k1",
    "elgamal"
  ],
  "main": "dist/index.js",
  "repository": "git@github.com:tuphan-dn/crypto-gears.git",
  "author": {
    "name": "Tu Phan",
    "email": "tuphan@gears.bot"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepare": "husky",
    "build": "pnpm clean && tsc",
    "test": "pnpm build && mocha --require ts-node/register -t 120000 test/**/*.test.ts",
    "test:watch": "nodemon --ext ts --watch src --watch test --exec 'pnpm test'",
    "ci:docs": "typedoc --includeVersion --excludePrivate --excludeProtected src/index.ts",
    "ci:release": "semantic-release",
    "clean": "rimraf dist docs"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@noble/curves": "^1.4.0",
    "@noble/hashes": "^1.1.5",
    "bn.js": "^5.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@ethereumjs/common": "^3.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.2",
    "@solana/web3.js": "^1.73.0",
    "@types/bn.js": "^5.1.1",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.18",
    "bs58": "^5.0.0",
    "chai": "^4.3.6",
    "ethers": "^6.11.1",
    "husky": "^9.0.11",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "semantic-release": "^23.0.6",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.3"
  }
}
